{
	"name": "demo-kit-feedback",
	"version": "0.0.3",
	"license": "MIT",
	"repository": {
		"type": "git",
		"url": "https://github.com/jycouet/kitql",
		"directory": "packages/modules/kit-feedback"
	},
	"scripts": {
		"dev": "env-cmd -x svelte-kit dev --port=\\$VITE_PORT",
		"kill": "npx kill-port \\$VITE_PORT",
		"build-kit": "svelte-kit build",
		"build": "tsc --project tsconfig.json",
		"gen": "yarn gen:modules && yarn gen:graphql",
		"gen:graphql": "graphql-codegen --config ./.graphqlrc.yaml -r dotenv/config",
		"gen:modules": "node ../../node_modules/@kitql/module-codegen/dist/index.js",
		"gen:style": "npx tailwindcss -c ./tailwind.config.cjs -o ./dist/app.postcss --minify",
		"package": "svelte-kit package && yarn gen:style",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "^1.20.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "1.0.0-next.304",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"autoprefixer": "^10.4.4",
		"cssnano": "^5.1.7",
		"daisyui": "^2.14.1",
		"dotenv": "^16.0.0",
		"env-cmd": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"graphql-modules": "^2.0.0",
		"postcss": "^8.4.12",
		"postcss-load-config": "^3.1.4",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.5",
		"svelte2tsx": "^0.5.8",
		"tailwind-scrollbar": "^1.3.1",
		"tailwindcss": "^3.0.23",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"dependencies": {
		"@envelop/core": "^2.3.1",
		"@envelop/graphql-jit": "^4.2.1",
		"@envelop/graphql-modules": "^3.3.1",
		"@graphql-yoga/common": "2.0.0",
		"@kitql/all-in": "^0.6.3",
		"@kitql/kit-feedback": "^0.0.5",
		"date-fns": "^2.28.0",
		"graphql": "16.3.0",
		"graphql-scalars": "^1.17.0",
		"reflect-metadata": "^0.1.13"
	},
	"peerDependencies": {},
	"resolutions": {
		"graphql": "16.3.0"
	},
	"type": "module",
	"publishConfig": {
		"directory": "dist",
		"access": "public"
	},
	"volta": {
		"node": "17.4.0",
		"yarn": "1.22.17"
	}
}
